
<%= form_for @group do |f| %>


<section class="wrapper">
        <div>
            <%= f.label :name, class: 'mylabel' %>
            <%= f.text_field :name %>

        </div>

          <div>
            <%= f.label :description, class: 'mylabel' %>
            <%= f.text_field :description %>

        </div>
        
          <div>
              <%= f.file_field :avatar %>
          </div>


        <div>
            <%= f.submit  %>
        </div>

</section>
<% end %>
